{
  "$schema": "https://github.com/vedantmgoyal9/vedantmgoyal9/raw/main/WinGetAutomation/schema.json",
  "PackageIdentifier": "AgileBits.1Password",
  "Source": [
    {
      "InvokeType": "RestMethod",
      "Uri": "https://c.1password.com/dist/1P/win6/updates-stable.json",
      "Method": "Get",
      "Headers": {},
      "Body": "",
      "UserAgent": ""
    }
  ],
  "PostResponseScript": "$Response = $Response.rules | Select-Object -Last 1",
  "Update": {
    "PackageVersion": "$Response.before",
    "InstallerUrls": "$Response.url"
  },
  "AdditionalInfo": {
    "ForceUpgrade": false,
    "VersionRegex": "[0-9.]+",
    "InstallerRegex": ".exe$"
  },
  "PostUpgradeScript": "",
  "Skip": {
    "Skip?": false,
    "Reason": null
  }
}
